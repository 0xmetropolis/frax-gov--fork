{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2022",
    "lib": ["es2017", "esnext.asynciterable"],
    "module": "NodeNext",
    "moduleResolution": "node",
    "declaration": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "exactOptionalPropertyTypes": false
  }
}
